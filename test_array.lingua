import { len, contains, index_of } from "std/array";
import { push, pop, shift, concat, reverse, sort, join, remove } from "std/array";

const nums = [1, 2, 3];
print(nums);
print(nums[0]);
print(nums[1:3]);
print(len(nums));
print(contains(nums, 2));
print(index_of(nums, 3));

const nums2 = push(nums, 4);
print(nums2);

const nums3 = pop(nums2);
print(nums3);

const nums4 = shift(nums);
print(nums4);

const merged = concat([1, 2], [3, 4]);
print(merged);

const rev = reverse(nums);
print(rev);

const sorted = sort([3, 1, 2]);
print(sorted);

const joined = join(["a", "b", "c"], ", ");
print(joined);

const removed = remove([10, 20, 30], 1);
print(removed);

const names: Array<string> = ["alice", "bob"];
print(names);

const empty: Array<int> = [];
print(len(empty));

print(nums == [1, 2, 3]);
print(nums == [1, 2]);
